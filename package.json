{
  "name": "static-html-boilerplate",
  "description": "Static HTML Boilerplate",
  "version": "0.6.0",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/prototypeinteractive/static-html-boilerplate.git"
  },
  "author": "Prototype Interactive",
  "license": "ISC",
  "homepage": "https://github.com/prototypeinteractive/static-html-boilerplate#readme",
  "private": true,
  "main": "server.js",
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "start": "node server.js",
    "lint": "node_modules/.bin/eslint assets/js --fix",
    "server": "cross-env NODE_ENV=development DEBUG=api nodemon server.js",
    "dev": "shx rm -rf public && webpack serve --config webpack.config.dev.js",
    "build": "shx rm -rf public && webpack --config webpack.config.prod.js",
    "deploy": "aws s3 cp public/ s3://[S3-BUCKET-ID] --recursive && aws cloudfront create-invalidation --distribution-id [CF-DISTRIBUTION-ID] --paths '/*'"
  },
  "dependencies": {
    "basic-auth": "^2.0.1",
    "bootstrap": "^5.1.3",
    "compression": "^1.7.4",
    "express": "^4.17.2",
    "helmet": "^5.0.2",
    "serve-static": "^1.14.2"
  },
  "devDependencies": {
    "@babel/core": "^7.17.2",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/eslint-plugin": "^7.16.5",
    "@babel/preset-env": "^7.16.11",
    "autoprefixer": "^10.4.2",
    "babel-loader": "^8.2.3",
    "browser-sync": "^2.27.7",
    "copy-webpack-plugin": "^10.2.4",
    "critical": "^4.0.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.6.0",
    "cssnano": "^5.0.17",
    "eslint": "^8.8.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-no-secrets": "^0.8.9",
    "eslint-plugin-no-unsanitized": "^4.0.1",
    "handlebars-webpack-plugin": "^2.2.1",
    "jshint": "^2.13.4",
    "mini-css-extract-plugin": "^2.5.3",
    "nodemon": "^2.0.15",
    "postcss": "^8.4.6",
    "postcss-loader": "^6.2.1",
    "sass": "^1.49.7",
    "sass-loader": "^12.4.0",
    "shx": "^0.3.4",
    "sitemap-webpack-plugin": "^1.1.1",
    "svg-sprite-loader": "^6.0.11",
    "svg4everybody": "^2.1.9",
    "svgo-loader": "^3.0.0",
    "webpack": "^5.68.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4",
    "webpack-remove-empty-scripts": "^0.7.3"
  }
}
